<!DOCTYPE html>
<html lang="ptbr">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table</title>
</head>
<body>
    <table>
        <thead>
            <tr>
               <% header.forEach(coll => { %>
                    <th>
                        <%= coll %>
                    </th>
                <% }) %> 
            </tr>
        </thead>
        <tbody>
            <% rows.forEach(row => { %>
                <tr>
                    <% row.forEach(element => { %>
                        <th>
                            <%= element %>
                        </th>
                    <% }) %> 
                </tr>
            <% }) %> 
        </tbody>
    </table>
</body>
</html>